{
  "name": "lambdasync",
  "version": "1.0.0",
  "description": "Easy sync between local code and AWS lambda functions",
  "main": "src/bin/index.js",
  "watch": {
    "build": {
      "patterns": [
        "bin",
        "src"
      ],
      "extensions": "js,json"
    }
  },
  "scripts": {
    "build": "npm run build:cli",
    "build:cli": "rollup -c",
    "watch": "npm-watch"
  },
  "bin": {
    "lambdasync": "bin/lambdasync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanderzon/lambda-deployer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fanderzon/lambda-deployer/issues"
  },
  "homepage": "https://github.com/fanderzon/lambda-deployer#readme",
  "dependencies": {
    "jsonfile": "2.3.1",
    "minimist": "1.2.0",
    "shelljs": "0.7.3"
  },
  "devDependencies": {
    "npm-watch": "0.1.5",
    "rollup-plugin-buble": "0.12.1",
    "rollup-plugin-commonjs": "3.3.1",
    "rollup-plugin-node-resolve": "2.0.0"
  }
}
