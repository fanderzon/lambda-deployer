{
  "name": "lambdasync",
  "version": "1.0.1",
  "description": "Easy sync between local code and AWS lambda functions",
  "main": "src/bin/index.js",
  "engines": {
    "node": ">=4.3"
  },
  "watch": {
    "build": {
      "patterns": [
        "bin",
        "src"
      ],
      "extensions": "js,json"
    }
  },
  "scripts": {
    "build": "npm run build:cli",
    "build:cli": "rollup -c",
    "watch": "npm-watch"
  },
  "bin": {
    "lambdasync": "bin/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanderzon/lambdasync.git"
  },
  "author": "Fredrik Andersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanderzon/lambdasync/issues"
  },
  "homepage": "https://github.com/fanderzon/lambdasync#readme",
  "dependencies": {
    "aws-sdk": "2.4.13",
    "bestzip": "1.1.3",
    "ini": "1.3.4",
    "minimist": "1.2.0",
    "rimraf": "2.5.4",
    "user-home": "2.0.0"
  },
  "devDependencies": {
    "npm-watch": "0.1.5",
    "rollup-plugin-buble": "0.12.1",
    "rollup-plugin-commonjs": "3.3.1",
    "rollup-plugin-json": "2.0.1",
    "rollup-plugin-node-resolve": "2.0.0"
  }
}
